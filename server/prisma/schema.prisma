generator client {
    provider = "prisma-client-js"
}

datasource db {
    provider  = "postgresql"
    url       = env("DATABASE_URL")
    directUrl = env("DATABASE_DIRECT_URL")
}

model User {
    id        String   @id @default(uuid())
    username  String
    pin       String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}
